# Google Cloud Storage
GCS_PROJECT_ID=
GCS_BUCKET_NAME=
# Path to service account key JSON file inside the container/host
GOOGLE_APPLICATION_CREDENTIALS=/workspace/backend/gcp-service-account.json

# Optional: override public base URL (leave empty to use https://storage.googleapis.com/<bucket>)
GCS_PUBLIC_BASE_URL=

# Optional defaults
DEFAULT_AVATAR_URL=
DEFAULT_PRODUCT_IMAGE_URL=

# Existing envs
PORT=3000
FRONTEND_URL=http://localhost:5173
